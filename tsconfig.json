{
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "ESNext",
    "target": "es2022",            
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
